// To find factorial 
  
 #include <stdio.h> 
 int main() 
 { 
     int d; 
     printf("Enter a number: "); 
     scanf("%d",&d); 
     printf("The factorial of %d is %d",d,fac(d)); 
 } 
 int fac(int d) 
 { 
     if(n<=2) 
     { 
         return d; 
     } 
     else 
     { 
     return (d*fac(d-1)); 
 } 
  
 } 
  
 ******************************************** 
  
 output  
  
 Enter num : 7  
 Fibonacci series : 0 1 1 2 3 5 8  
  
  
 ******************************************** 
  
 // Fibonacci series without function 
  
 #include<stdio.h> 
 int main(){ 
     int a=0,b=1,d,l; 
     printf("Enter num : "); 
     scanf("%d",&d); 
     printf("Fibonacci series : "); 
     printf("%d %d ",a,b); 
     for(int i=1;i<=d-2;i++){ 
         l=a+b; 
         a=b; 
         b=c; 
         printf("%d ",l); 
     } 
 } 
  
 ******************************************** 
  
 output 
  
 Enter num : 5  
 Fibonacci series : 0 1 1 2 3 5  
  
 ******************************************** 
  
 // Fibonacci series with function 
  
 #include<stdio.h> 
 int main() 
 { 
     int a=0,b=1,d; 
     printf("Enter num : "); 
     scanf("%d",&d); 
     printf("Fibonacci series : "); 
     for(int i=0;i<=d;i++) 
     { 
         printf("%d ",fib(i)); 
     } 
 } 
 int fib(int d) 
 { 
     if(n<=1){ 
         return d; 
     } 
     else { 
         return fib(d-1)+fib(d-2); 
     } 
 } 
  
 ******************************************** 
  
 output 
  
 Enter num : 7
 Fibonacci series : 0 1 1 2 3 5 8 
